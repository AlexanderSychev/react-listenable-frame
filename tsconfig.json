{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "target": "ES5",
    "baseUrl": "./src",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "strict": true,
    "jsx": "react",
    "lib": [
      "es2015",
      "DOM"
    ]
  }
}
